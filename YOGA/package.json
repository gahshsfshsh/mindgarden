  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:export": "next build && next export",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true npm run build",
    "generate-content": "ts-node scripts/generate-content.ts",
    "download-media": "node scripts/download-media-content.js",
    "generate-audio": "node scripts/generate-placeholder-audio.js",
    "download-enhanced": "node scripts/download-free-content-enhanced.js",
    "setup": "npm install && npm run generate-audio",
    "setup:full": "npm install && npm run download-enhanced",
    "mobile:build": "npm run build && npx cap sync",
    "mobile:ios": "npm run mobile:build && npx cap open ios",
    "mobile:android": "npm run mobile:build && npx cap open android"
  },
